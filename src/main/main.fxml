<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#backClick" text="back" />
            <HBox alignment="CENTER_RIGHT" spacing="10.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="welcomeLabel" text="welcome" />
                  <Label fx:id="usernameLabel" text="username" />
                  <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutClick" text="logout" />
               </children>
            </HBox>
         </children>
      </HBox>
      <Pane fx:id="contentPane" />
      <HBox alignment="CENTER_RIGHT">
         <children>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#exitClick" text="exit" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
